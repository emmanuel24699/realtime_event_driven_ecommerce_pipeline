{
  "family": "lab6-validation-task",
  "networkMode": "awsvpc",
  "containerDefinitions": [
    {
      "name": "validation-container",
      "image": "985539772768.dkr.ecr.us-east-1.amazonaws.com/lab6-ecommerce-validation:latest",
      "essential": true,
      "logConfiguration": {
        "logDriver": "awslogs",
        "options": {
          "awslogs-group": "/ecs/lab6-ecommerce",
          "awslogs-region": "us-east-1",
          "awslogs-stream-prefix": "validation"
        }
      }
    }
  ],
  "requiresCompatibilities": ["FARGATE"],
  "executionRoleArn": "arn:aws:iam::985539772768:role/lab6-ecs-task-role",
  "taskRoleArn": "arn:aws:iam::985539772768:role/lab6-ecs-task-role",
  "cpu": "256",
  "memory": "512"
}
