{
  "family": "lab6-staging-task",
  "containerDefinitions": [
    {
      "name": "staging-container",
      "image": "985539772768.dkr.ecr.us-east-1.amazonaws.com/lab6-ecommerce-staging:latest",
      "essential": true,
      "environment": [],
      "logConfiguration": {
        "logDriver": "awslogs",
        "options": {
          "awslogs-group": "/ecs/lab6-ecommerce/staging",
          "awslogs-region": "us-east-1",
          "awslogs-stream-prefix": "staging",
          "awslogs-create-group": "true"
        }
      },
      "memory": 2048,
      "cpu": 1024
    }
  ],
  "executionRoleArn": "arn:aws:iam::985539772768:role/lab6-ecs-task-execution-role",
  "taskRoleArn": "arn:aws:iam::985539772768:role/lab6-ecs-task-role",
  "networkMode": "awsvpc",
  "requiresCompatibilities": ["FARGATE"],
  "cpu": "1024",
  "memory": "2048"
}
