{
  "family": "lab6-dynamodb-task",
  "networkMode": "awsvpc",
  "containerDefinitions": [
    {
      "name": "dynamodb-container",
      "image": "985539772768.dkr.ecr.us-east-1.amazonaws.com/lab6-ecommerce-dynamodb:latest",
      "essential": true,
      "logConfiguration": {
        "logDriver": "awslogs",
        "options": {
          "awslogs-group": "/ecs/lab6-ecommerce",
          "awslogs-region": "us-east-1",
          "awslogs-stream-prefix": "dynamodb"
        }
      },
      "environment": [{ "name": "SPARK_LOCAL_IP", "value": "127.0.0.1" }],
      "command": ["python", "write_dynamodb.py"]
    }
  ],
  "requiresCompatibilities": ["FARGATE"],
  "executionRoleArn": "arn:aws:iam::985539772768:role/lab6-ecs-task-role",
  "taskRoleArn": "arn:aws:iam::985539772768:role/lab6-ecs-task-role",
  "cpu": "512",
  "memory": "1024"
}
